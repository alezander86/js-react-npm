{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "flow-bin": "^0.188.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "CI=true react-scripts test --coverage --watchAll=false --testPathIgnorePatterns=node_modules",
    "eject": "react-scripts eject",
    "flow": "flow",
    "flow:check": "pnpm exec flow check",
    "prepare:artifacts": "pnpm run build:prod",
    "prepare:validate": "pnpm run test:coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
